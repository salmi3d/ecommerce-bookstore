{
  "connection": "default",
  "collectionName": "books",
  "info": {
    "name": "book"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "price": {
      "type": "decimal"
    },
    "genre": {
      "model": "genre",
      "via": "books"
    }
  }
}
